create database opilaneOdinokov;
use opilaneOdinokov;

Create table Isik(
id int primary key AUTO_INCREMENT,
eesnimi varchar(50),
perenimi varchar(50) not null,
sugu char(1),
sünnikupäev date,
aaddress varchar(100),
email varchar(50)
);

Create table Õppeaine(
id int primary key AUTO_INCREMENT,
nimi varchar(100),
vastutav_õpetaja integer,
aine_kestus varchar(50)
);

Create table Õppimine(
id int primary key AUTO_INCREMENT,
isiku_id integer,
õppeaine_id integer,
hinne integer,
);

